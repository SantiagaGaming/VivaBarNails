@page
@model AllNailsOrders

<h4 style="text-align:center">Наши работы</h4>

     <div class="well text-center">
    <div class="row">
@foreach (var item in @Model.Orders)
{
    <div class="col-md-4">
      <h5 class="card-title">@item.NailName</h5>
       <img class="card-img-top masterImage" src="@item.Img" alt="Card image cap">
       </div>
}
</div>
</div>


  

    


